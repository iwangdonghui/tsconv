{
  "functions": {
    "api/convert.ts": {
      "maxDuration": 10
    },
    "api/now.ts": {
      "maxDuration": 10
    },
    "api/batch.ts": {
      "maxDuration": 15
    },
    "api/health.ts": {
      "maxDuration": 5
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
